{
  "agent": {
    "model": "anthropic/claude-sonnet-4",
    "thinking": "medium"
  },
  "gateway": {
    "port": 18789,
    "host": "0.0.0.0"
  },
  "workspace": {
    "root": "~/aethervault-workspace"
  },
  "sandbox": {
    "mode": "non-main"
  },
  "security": {
    "dm_policy": "pairing",
    "allowlist": []
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "${TELEGRAM_BOT_TOKEN}"
    },
    "discord": {
      "enabled": false,
      "token": "${DISCORD_BOT_TOKEN}"
    },
    "slack": {
      "enabled": false,
      "bot_token": "${SLACK_BOT_TOKEN}",
      "app_token": "${SLACK_APP_TOKEN}"
    },
    "whatsapp": {
      "enabled": false
    }
  },
  "voice": {
    "enabled": false,
    "elevenlabs_api_key": "${ELEVENLABS_API_KEY}"
  }
}
