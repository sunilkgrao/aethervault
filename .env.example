# Django
DJANGO_SECRET_KEY=change-me
DJANGO_DEBUG=1
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1

# Abuse controls
TG_THROTTLE_AGENT_REGISTER=20/hour
TG_MAX_CLAIMS_PER_X_HANDLE=3

# Database
